if [ "$isFirstRun" == "true" ]
then
   local IFS=$(echo -en "\n\b")
   cronVars="(set | /bin/grep "VAR_cron_")"
   for cronVar in $cronVars
   do
      name=${cronVar%=*}
      cron=${cronVar#*=}
      echo "$cron /backup/scripts/$name" >> crontab
   done
   crontab -u $VAR_LINUX_USER crontab
   /bin/rm crontab
fi
